remote SERVER_IP PORT 
client
dev tun

comp-lzo
proto udp
resolv-retry infinite
tls-client

ca [inline]
cert [inline]
key [inline]
tls-auth [inline] 1

remote-cert-tls server
ns-cert-type server
cipher AES-256-CBC

nobind
auth-nocache
persist-key
persist-tun

verb 4
pull

script-security 2

up  ~/.openvpn/update-resolv-conf # Linux DNS
down ~/.openvpn/update-resolv-conf # Linux DNS

